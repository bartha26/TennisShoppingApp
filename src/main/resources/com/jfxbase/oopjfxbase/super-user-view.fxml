<AnchorPane fx:id="anchorPane" prefHeight="797.0" prefWidth="991.0" stylesheets="@super-user-style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jfxbase.oopjfxbase.controllers.SuperUserController">


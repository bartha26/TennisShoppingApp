<AnchorPane fx:id="anchorPane" prefHeight="866.0" prefWidth="1573.0" stylesheets="@hello-style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jfxbase.oopjfxbase.controllers.HelloController">
   <children>

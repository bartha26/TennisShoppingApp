      <Button fx:id="rackets" layoutX="12.0" layoutY="52.0" mnemonicParsing="false" onAction="#onRacketsButtonClick" text="Rackets" />
      <Button fx:id="shorts" layoutX="100.0" layoutY="52.0" mnemonicParsing="false" onAction="#onShortsButtonClick" text="Shorts" />
      <Button fx:id="shirts" layoutX="175.0" layoutY="52.0" mnemonicParsing="false" onAction="#onShirtButtonClick" text="T-Shirts" />
      <Button fx:id="shoes" layoutX="261.0" layoutY="52.0" mnemonicParsing="false" onAction="#onShoesButtonClick" text="Shoes" />
      <Button fx:id="reviews" layoutX="328.0" layoutY="52.0" mnemonicParsing="false" onAction="#onReviewButtonClick" text="Reviews" />
      <TextField fx:id="nameField" layoutX="49.0" layoutY="130.0" promptText="Name..." />
      <TextField fx:id="quantityField" layoutX="242.0" layoutY="185.0" promptText="quantity..." />
      <TableView fx:id="table" layoutX="473.0" layoutY="-1.0" prefHeight="858.0" prefWidth="467.0" />
      <Text layoutX="14.0" layoutY="117.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please introduce the name of the product" />
      <TextField fx:id="colorField" layoutX="242.0" layoutY="232.0" promptText="color..." />
      <Button fx:id="addToCart" layoutX="281.0" layoutY="353.0" mnemonicParsing="false" onAction="#onAddToCartButtonClick" text="AddToCart" />
      <TextField fx:id="minPriceField" layoutX="50.0" layoutY="232.0" promptText="minPrice..." />
      <TextField fx:id="maxPriceField" layoutX="50.0" layoutY="185.0" promptText="maxPrice..." />
      <TextField fx:id="sizeField" layoutX="242.0" layoutY="273.0" promptText="size/weight..." />
      <TableView fx:id="tableSecond" layoutY="388.0" prefHeight="410.0" prefWidth="187.0" />
      <TableView fx:id="tableThird" layoutX="185.0" layoutY="388.0" prefHeight="409.0" prefWidth="287.0" />
      <TextField fx:id="type" layoutX="243.0" layoutY="130.0" promptText="product type..." />
      <Text layoutX="251.0" layoutY="116.0" strokeType="OUTSIDE" strokeWidth="0.0" text="One of the 4 types above" />
      <Text fx:id="message" layoutX="298.0" layoutY="318.0" strokeType="OUTSIDE" strokeWidth="0.0" text="HELLO!" />
      <Button fx:id="seeCart" layoutX="85.0" layoutY="301.0" mnemonicParsing="false" onAction="#onSeeCartButtonClick" text="SeeCart" />
      <Button fx:id="formerOrders" layoutX="68.0" layoutY="340.0" mnemonicParsing="false" onAction="#onSeeFormerOrders" text="FormerOrders" />
   </children>
</AnchorPane>

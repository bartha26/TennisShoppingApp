<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" prefHeight="653.0" prefWidth="783.0" stylesheets="@pay-style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jfxbase.oopjfxbase.controllers.PaymentController">
   <children>
      <TextField fx:id="cardNumber" layoutX="42.0" layoutY="71.0" prefHeight="26.0" prefWidth="230.0" promptText="CardNumber..." />
      <Button fx:id="back" layoutY="2.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back" />
      <TextField fx:id="name" layoutX="42.0" layoutY="115.0" prefHeight="26.0" prefWidth="230.0" promptText="CardHolderName..." />
      <DatePicker fx:id="datePicker" layoutX="42.0" layoutY="163.0" promptText="ExpirationDate..." />
      <TextField fx:id="cvv" layoutX="42.0" layoutY="206.0" promptText="CVV..." />
      <TextField fx:id="shippingAddress" layoutX="43.0" layoutY="253.0" prefHeight="26.0" prefWidth="244.0" promptText="ShippingAddress..." />
      <Button fx:id="finishPayment" layoutX="70.0" layoutY="314.0" mnemonicParsing="false" onAction="#onFinishPayButtonClick" text="FinishPayment" />
      <Text fx:id="message" layoutX="101.0" layoutY="301.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Hello!" />
   </children>
</AnchorPane>
